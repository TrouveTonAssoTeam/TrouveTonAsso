h1>Créer un nouveau profil d'association</h1>

<%= form_for @association_profile do |f| %>
  <!-- Champs du formulaire pour la création du profil -->
  <div class="field">
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>

  <!-- Ajoutez d'autres champs au besoin -->

  <div class="actions">
    <%= f.submit "Créer le profil" %>
  </div>
<% end %>